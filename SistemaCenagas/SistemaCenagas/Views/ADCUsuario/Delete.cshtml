@model SistemaCenagas.Models.ADC

@{
    ViewData["Title"] = "Delete";
}

<div class="modal fade" tabindex="-1" role="dialog" id="eliminarADCUsuario-modal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Eliminar ADC</h5>
                <a asp-action="Index" class="close btn btn-round">
                    <i class="material-icons">close</i>
                </a>
            </div>
            <!--end of modal head-->
            <div class="modal-body">

                <form asp-action="Delete">
                    <div class="card card-note">
                        <div class="card-body">
                            <p>¿Estás seguro de eliminar este ADC?</p>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <input type="hidden" asp-for="Id_ADC" />
                        <button role="button" class="btn btn-danger" type="submit">
                            Eliminar
                        </button>
                        <a class="btn btn-dark" asp-action="Index">
                            Cancelar
                        </a>
                    </div>

                </form>

            </div>
        </div>
    </div>
</div>
