@model IEnumerable<SistemaCenagas.Models.ADC>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id_Proyecto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_ResponsableADC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Suplente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Lider)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Actualizacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Proyecto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_ResponsableADC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Suplente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Lider)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Actualizacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_ADC">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_ADC">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_ADC">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
