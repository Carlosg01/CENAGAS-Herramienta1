@model IEnumerable<SistemaCenagas.Models.Asignacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id_Empleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Proyecto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Residencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_alta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_baja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Empleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Proyecto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Residencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_alta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_baja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Asignacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Asignacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Asignacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
